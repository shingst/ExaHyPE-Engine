[submodule "Submodules/Peano"]
	path = Submodules/Peano
	url = https://gitlab.lrz.de/hpcsoftware/Peano.git
	ignore = untracked # .o from compilation
[submodule "Submodules/jinja"]
	path = Submodules/jinja
	url = https://github.com/pallets/jinja.git
[submodule "Submodules/markupsafe"]
	path = Submodules/markupsafe
	url = https://github.com/pallets/markupsafe.git
[submodule "Submodules/libxsmm"]
	path = Submodules/libxsmm
	url = https://github.com/hfp/libxsmm.git
	branch = release
	ignore = dirty #deleted documentation and sample + build
[submodule "Submodules/attrs"]
	path = Submodules/attrs
	url = https://github.com/python-attrs/attrs.git
[submodule "Submodules/pyrsistent"]
	path = Submodules/pyrsistent
	url = https://github.com/tobgu/pyrsistent.git
[submodule "Submodules/jsonschema"]
	path = Submodules/jsonschema
	url = https://github.com/Julian/jsonschema.git
	ignore = dirty #modified __init__.py + __pycache__
[submodule "Submodules/six"]
	path = Submodules/six
	url = https://github.com/benjaminp/six.git
[submodule "six"]
	path = six
	url = git@github.com:benjaminp/six.git
